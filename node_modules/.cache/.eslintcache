p/wbdv-s21-xinyue-dang-client-react/src/reducers/widget-reducer.js",["190"],"import React from 'react'\n\nconst initialState = {\n    widgets: [],\n}\n\nconst widgetReducer = (state = initialState, action) => {\n    switch (action.type) {\n        case 'CREATE_WIDGET':\n            const newState = {\n                ...state,\n                widgets: [...state.widgets, action.widget],\n            }\n            return newState\n        case 'FIND_ALL_WIDGETS_FOR_TOPIC':\n            const newStateA = {\n                ...state,\n                widgets: action.widgets,\n            }\n            return newStateA\n        case 'FIND_WIDGET':\n        case 'UPDATE_WIDGET':\n            const newStateU = {\n                widgets: state.widgets.map((w) => {\n                    if (w.id === action.widgetToUpdate.id) {\n                        return action.widgetToUpdate\n                    } else {\n                        return w\n                    }\n                }),\n            }\n            return newStateU\n        case 'DELETE_WIDGET':\n            const newStateD = {\n                widgets: state.widgets.filter((w) => {\n                    if (w.id === action.widgetToDelete.id) {\n                        return false\n                    } else {\n                        return true\n                    }\n                }),\n            }\n            return newStateD\n        case 'CLEAR_WIDGET':\n            const newStateC = {\n                ...state,\n                widgets: [],\n            }\n            return newStateC\n        default:\n            return state\n    }\n}\n\nexport default widgetReducer\n","/Users/xinyue/Desktop/wbdv-s21-xinyue-dang-client-react/src/services/widget-service.js",[],"/Users/xinyue/Desktop/wbdv-s21-xinyue-dang-client-react/src/components/widgets/optional-element.js",["191","192","193","194","195"],"import React, { useState } from 'react'\n\nconst OptionalElement = ({ item, setItem }) => {\n    return 