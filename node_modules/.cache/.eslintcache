         {\n                !editing &&\n            <>\n                <a className=\"nav-link\" href=\"#\">\n                    {item.title}\n                </a>\n                <i onClick = {() => setEditing(true)} className=\"fas fa-edit\"></i>\n            </>\n            }\n            {editing &&\n            <>\n                <input \n                    onChange={(e) => \n                        {setItemCache({\n                            ...itemCache, \n                            title: e.target.value})}}\n                    value={itemCache.title}/>\n                <i onClick={()=>\n                    {setEditing(false)\n                    updateItem(itemCache)\n                    }} className=\"fas fa-check\"></i>\n                <i onClick={()=>deleteItem(item)} className=\"fas fa-times\"></i>\n            </>\n            }\n        </>\n    )\n}\n\nexport default EditableItem",{"ruleId":"102","replacedBy":"103"},{"ruleId":"104","replacedBy":"105"},{"ruleId":"106","severity":1,"message":"107","line":4,"column":8,"nodeType":"108","messageId":"109","endLine":4,"endColumn":20},{"ruleId":"110","severity":1,"message":"111","line":8,"column":3,"nodeType":"112","messageId":"113","endLine":10,"endColumn":4},{"ruleId":"106","severity":1,"message":"114","line":3,"column":16,"nodeType":"108","messageId":"109","endLine":3,"endColumn":21},{"ruleId":"115","severity":1,"message":"116","line":42,"column":33,"nodeType":"117","endLine":46,"endColumn":34},{"ruleId":"115","severity":1,"message":"116","line":51,"column":33,"nodeType":"117","endLine":51,"endColumn":75},{"ruleId":"115","severity":1,"message":"116","line":56,"column":33,"nodeType":"117","endLine":56,"endColumn":75},{"ruleId":"115","severity":1,"message":"116","line":61,"column":33,"nodeType":"117","endLine":61,"endColumn":75},{"ruleId":"115","severity":1,"message":"116","line":66,"column":33,"nodeType":"117","endLine":66,"endColumn":75},{"ruleId":"115","severity":1,"message":"116","line":71,"co