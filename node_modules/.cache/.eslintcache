s\">\n                <CourseManager/>\n            </Route>\n        </div>\n      </BrowserRouter>\n  );\n}\n\nexport default App;\n","/Users/xinyue/Desktop/wbdv-s21-xinyue-dang-client-react/src/reportWebVitals.js",[],"/Users/xinyue/Desktop/wbdv-s21-xinyue-dang-client-react/src/components/course-table/course-table.js",["56"],"import React from 'react'\nimport CourseRow from \"./course-row\";\nimport {Link} from \"react-router-dom\";\n\nexport default class CourseTable\n  extends React.Component {\n\n  constructor(props) {\n    super(props)\n  }\n\n render() {\n     return(\n       <div>\n         <table className=\"table\">\n         <thead>\n            <tr>\n                <th>Title</th>\n                <th className=\"d-none d-md-table-cell\">Owned By</th>\n                <th className=\"d-none d-lg-table-cell\">Last Modified</th>\n                <th>\n                    <i className=\"fas fa-lg fa-folder\"></i>\n                    &nbsp;\n                    <i className=\"fas fa-lg fa-sort-alpha-up\"></i>\n                    &nbsp;\n                    <Link to=\"/courses/grid\">\n                        <i className=\"fas fa-lg fa-th\"></i>\n                    </Link>\n                </th>\n            </tr>\n         </thead>\n         <tbody>\n           {\n             this.props.courses.map((course) =>\n               <CourseRow\n                 updateCourse={this.props.updateCourse}\n                 deleteCourse={this.props.deleteCourse}\n                 key={course._id}\n                 course={course}\n                 title={course.title}\n                 owner={course.owner}\n                 lastModified={course.lastModified}\n               />)\n           }\n         </tbody>\n         </table>\n       </div>\n     )\n   }\n }\n","/Users/xinyue/Desktop/wbdv-s21-xinyue-dang-client-react/src/components/course-table/course-row.js",[],"/Users/xinyue/Desktop/wbdv-s21-xinyue-dang-client-react/src/components/course-manager.js",["57"],"import React from 'react'\nimport CourseTable from \"./course-table/course-table\";\nimport CourseGrid from \"./course-grid/course-grid\";\nimport CourseEditor from \"./course-editor/course-editor\";\nimport \"./course-manager.css\";\nimport {Link, Route} from \"react-router-dom\";\nimport courseService from \"../services/course-service\";\n\nclass CourseManager extends React.Component {\n  state = {\n    courses: [],\n    newInput:''\n  }\n  updateCourse = (course) => {\n     courseService.updateCourse(course._id, course)\n         .then(status => this.setState((prevState) => ({\n           ...prevState,\n           courses: prevState.courses.map(\n               (c) => c._id === course._id ? course : c)\n         })))\n      }\n\n  componentDidMount = () =>\n      courseService.findAllCourses()\n          .then(courses => {\n          this.setState({...this.state, courses:courses})\n          console.log(courses)\n        })\n  addCourse = () => {\n    const newCourse = {\n      title: this.state.newInput,\n      owner: \"me\",\n      lastModified: \"1/1/2021\"\n    }\n    this.setState({newInput:''})\n    courseService.createCourse(newCourse)\n            .then(course => this.setState(\n                (prevState) => ({\n                  ...prevState,\n                  courses: [\n                      ...prevState.courses,\n                      course\n                  ]\n                })))\n  }\n\n  deleteCourse = (courseToDelete) => {\n      courseService.deleteCourse(courseToDelete._id)\n          .then(status => {\n            this.setState((prevState) => ({\n                ...prevState,\n                courses: prevState.courses.filter\n                  (course => course !== courseToDelete)\n            }))\n          })\n\n    }\n  handleInputChange = (event) => {\n      this.setState({\n        newInput: event.target.value\n      });\n    }\n\n  render() {\n    return(\n          <div>\n              <div className=\"headers\">\n                      <div className=\"wbdv-sticky-top wbdv-padding-5px\">\n                          <div className=\"row\">\n                              <div className=\"col-1 nav-btn\">\n                                  <i className=\"fa fa-bars fa-2x\"></i>\n                              </div>\n                              <div className=\"col-3 page-header d-none d-sm-block\">\n                                  <h5>Course Manager</h5>\n                              </div>\n                              <div className=\"col-7\">\n                                  <input className=\"form-control\" placeholder=\"New Course Title\"\n                                  onChange={this.handleInputChange}\n                                  value={this.state.newInput}/>\n                              </div>\n                              <div className=\"col-1 add-new\">\n                                  <i className=\"fa fa-plus fa-2x\" onClick={this.addCourse}></i>\n                              </div>\n                          </div>\n                      </div>\n              </div>\n              <button className=\"btn add-course-btn rounded-circle\">\n                  <i className=\"fa fa-plus fa-2x\" onClick={this.addCourse}></i>\n              </button>\n              <div className=\"courses\">\n                  <Route path=\"/courses/table\">\n                    <CourseTable\n                        updateCourse={this.updateCourse}\n                        deleteCourse={this.deleteCourse}\n                        courses={this.state.courses}/>\n                  </Route>\n                  <Route path=\"/courses/grid\">\n                    <CourseGrid\n                        updateCourse={this.updateCourse}\n                        deleteCourse={this.deleteCourse}\n                        courses={this.state.courses}/>\n                  </Route>\n                  <Route path=\"/courses/editor\"\n                        render={(props) => <CourseEditor {...props}/>}>\n                  </Route>\n              </div>\n            </div>\n    )\n  }\n}\n\nexport default CourseManager\n","/Users/xinyue/Desktop/wbdv-s21-xinyue-dang-client-react/src/components/course-grid/course-grid.js",[],"/Users/xinyue/Desktop/wbdv-s21-xinyue-dang-client-react/src/components/course-grid/course-card.js",[],"/Users/xinyue/Desktop/wbdv-s21-xinyue-dang-client-react/src/components/course-editor/course-editor.js",["58","59","60","61","62","63","64","65","66","67","68"],"import React from 'react'\nimport \"./course-editor.css\"\n\nconst CourseEditor = () =>\n  <div>\n      <div className=\"wbdv-sticky-top wbdv-padding-5px\">\n          <div className=\"row\">\n              <div className=\"col-1 nav-btn\">\n                  <i className=\"fa fa-times fa-2x\"></i>\n              </div>\n              <div className=\"col-3 page-header\">\n                  <h5>CS5610-WebDev</h5>\n              </div>\n              <div className=\"col-7\">\n                  <ul className=\"nav nav-tabs\">\n                      <li className=\"nav-item\">\n                          <a aria-current=\"page\" className=\"nav-link nav-tabs active\" href=\"#\">\n                              Build\n                          </a>\n                      </li>\n                      <li className=\"nav-item\">\n                          <a className=\"nav-link nav-tabs\" href=\"#\">Pages</a>\n                      </li>\n                      <li className=\"nav-item\">\n                          <a className=\"nav-link nav-tabs\" href=\"#\">Theme</a>\n                      </li>\n                      <li className=\"nav-item\">\n                          <a className=\"nav-link nav-tabs\" href=\"#\">Store</a>\n                      </li>\n                      <li className=\"nav-item\">\n                          <a className=\"nav-link nav-tabs\" href=\"#\">Apps</a>\n                      </li>\n                      <li className=\"nav-item\">\n                          